<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MainBtn">
        <attr name="btnText" format="string" />
        <attr name="btnState" format="enum">
            <enum name="loading" value="1" />
            <enum name="noLoading" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="movieWorldEditText">
        <attr name="boxError" format="string"/>
        <attr name="boxLabel" format="string"/>
        <attr name="boxImage" format="reference"/>
        <attr name="setInputType" format="integer"/>
        <attr name="boxDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="LoadingImage">
        <attr name="isLoading" format="boolean"/>
        <attr name="enableLoading" format="boolean"/>
        <attr name="initialState" format="enum">
            <enum name="loading" value="1"/>
            <enum name="notLoading" value="0"/>
        </attr>
    </declare-styleable>
</resources>